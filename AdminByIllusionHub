-- Glow Effect
local ToggleGlow = Instance.new("ImageLabel")
ToggleGlow.Size = UDim2.new(1, 20, 1, 20)
ToggleGlow.Position = UDim2.new(0.5, 0, 0.5, 0)
ToggleGlow.AnchorPoint = Vector2.new(0.5, 0.5)
ToggleGlow.BackgroundTransparency = 1
ToggleGlow.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"
ToggleGlow.ImageColor3 = Color3.fromRGB(0, 255, 100)
ToggleGlow.ImageTransparency = 0.7
ToggleGlow.ZIndex = 9
ToggleGlow.Parent = ToggleButton

-- Pulse Animation
spawn(function()
    while true do
        TweenService:Create(ToggleGlow, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {
            ImageTransparency = 0.9,
            Size = UDim2.new(1, 30, 1, 30)
        }):Play()
        wait(1)
    end
end)

